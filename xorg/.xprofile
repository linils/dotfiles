xrandr \
  --output DP-0 --primary --mode 1920x1080 --rate 144 --pos 0x420 \
  --output HDMI-0 --mode 1920x1080 --rate 144 --rotate right --pos 1920x0
xrdb ~/.Xresources
localectl set-x11-keymap se
setxkbmap -option caps:escape
xsetroot -cursor_name left_ptr
xset r rate 300 50

feh --bg-fill ~/.local/share/wallpaper

killall udiskie
udiskie &

killall nm-applet
nm-applet &

export $(dbus-launch)

killall picom
picom --daemon --no-frame-pacing &

killall blugon
blugon &
